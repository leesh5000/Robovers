{
  "name": "robovers-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "pnpm --filter @robovers/frontend dev",
    "build": "pnpm --filter @robovers/frontend build",
    "start": "pnpm --filter @robovers/frontend start",
    "lint": "pnpm -r lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "pnpm -r test",
    "test:coverage": "pnpm -r test:coverage"
  },
  "devDependencies": {
    "prettier": "^3.2.4"
  }
}